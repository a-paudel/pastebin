#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/@sveltejs+kit@1.22.3_svelte@4.0.5_vite@4.4.4/node_modules/@sveltejs/kit/node_modules:/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/@sveltejs+kit@1.22.3_svelte@4.0.5_vite@4.4.4/node_modules/@sveltejs/node_modules:/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/@sveltejs+kit@1.22.3_svelte@4.0.5_vite@4.4.4/node_modules:/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/@sveltejs+kit@1.22.3_svelte@4.0.5_vite@4.4.4/node_modules/@sveltejs/kit/node_modules:/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/@sveltejs+kit@1.22.3_svelte@4.0.5_vite@4.4.4/node_modules/@sveltejs/node_modules:/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/@sveltejs+kit@1.22.3_svelte@4.0.5_vite@4.4.4/node_modules:/home/ankur/Projects/Node/pastebin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../@sveltejs/kit/svelte-kit.js" "$@"
fi
